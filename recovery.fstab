# mount point   fstype  device					device2			flags
/system		ext4	/dev/block/platform/soc/624000.ufshc/by-name/system
/cache		ext4	/dev/block/platform/soc/624000.ufshc/by-name/cache
/data		ext4	/dev/block/platform/soc/624000.ufshc/by-name/userdata
/boot		emmc	/dev/block/platform/soc/624000.ufshc/by-name/boot
/recovery 	emmc	/dev/block/platform/soc/624000.ufshc/by-name/recovery
/misc 		emmc	/dev/block/platform/soc/624000.ufshc/by-name/misc
/cust 		ext4 	/dev/block/platform/soc/624000.ufshc/by-name/cust
/efs1           emmc    /dev/block/platform/soc/624000.ufshc/by-name/modemst1  flags=backup=1;display=EFS
/efs2           emmc    /dev/block/platform/soc/624000.ufshc/by-name/modemst2  flags=backup=1;subpartitionof=/efs1

# Removable storage
/storage/usbotg	vfat	/devices/soc/6a00000.ssusb/6a00000.dwc3/xhci-hcd.0.auto/usb1*	flags=display="USB-OTG"
